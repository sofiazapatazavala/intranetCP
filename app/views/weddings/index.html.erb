<p id="notice"><%= notice %></p>

<h1>Weddings</h1>

<table>
  <thead>
    <tr>
      <th>Created at</th>
      <th>Updated at</th>
      <th>Nombre1</th>
      <th>Nombre2</th>
      <th>Fecha matri</th>
      <th>Celular</th>
      <th>Mail</th>
      <th>Origen</th>
      <th>Url vals</th>
      <th>Comentarios</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @weddings.each do |wedding| %>
      <tr>
        <td><%= wedding.created_at %></td>
        <td><%= wedding.updated_at %></td>
        <td><%= wedding.nombre1 %></td>
        <td><%= wedding.nombre2 %></td>
        <td><%= wedding.fecha_matri %></td>
        <td><%= wedding.celular %></td>
        <td><%= wedding.mail %></td>
        <td><%= wedding.origen %></td>
        <td><%= wedding.url_vals %></td>
        <td><%= wedding.comentarios %></td>
        <td><%= link_to 'Show', wedding %></td>
        <td><%= link_to 'Edit', edit_wedding_path(wedding) %></td>
        <td><%= link_to 'Destroy', wedding, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Wedding', new_wedding_path %>
